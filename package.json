{
  "name": "8bit-agent-town",
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "dev": "concurrently \"npm run dev:client\" \"npm run dev:server\"",
    "dev:client": "vite",
    "dev:server": "tsx watch src/server/index.ts",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "test": "vitest",
    "test:e2e": "playwright test",
    "test:e2e:fullstack": "playwright test -c playwright.fullstack.config.ts",
    "test:e2e:headed": "playwright test --headed",
    "stress": "tsx src/server/testing/runStress.ts --profile baseline",
    "stress:llm": "tsx src/server/testing/runStress.ts --profile llm",
    "stress:profiles": "tsx src/server/testing/runStress.ts --profile both",
    "stress:long": "STRESS_DAYS=10 tsx src/server/testing/runStress.ts --profile both",
    "quality:conversations": "tsx src/server/testing/runConversationQuality.ts"
  },
  "dependencies": {
    "express": "^4.18.0",
    "p-queue": "^8.0.0",
    "phaser": "^3.70.0",
    "ws": "^8.16.0",
    "zod": "^3.23.0"
  },
  "devDependencies": {
    "@playwright/test": "^1.58.1",
    "@types/express": "^4.17.0",
    "@types/node": "^20.0.0",
    "@types/ws": "^8.5.0",
    "concurrently": "^8.0.0",
    "tsx": "^4.0.0",
    "typescript": "^5.3.0",
    "vite": "^5.0.0",
    "vitest": "^1.0.0"
  }
}
